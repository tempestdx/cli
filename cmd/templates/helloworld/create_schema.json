{
    "$schema": "https://developer.tempestdx.com/schema/v1/tempest-app-schema.json",
    "$id": "https://schema.tempestdx.io/privateapps/myapp/create.json",
    "type": "object",
    "properties": {
        "name": {
            "title": "Name",
            "type": "string",
            "description": "The name of the resource.",
            "pattern": "^[A-Za-z0-9_.-]+$",
            "examples": [
                "my-resource"
            ]
        }
    },
    "required": [
        "name"
    ],
    "additionalProperties": false
}
